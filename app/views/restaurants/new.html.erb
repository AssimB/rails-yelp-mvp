<br>
<div class="container" style="width: 600px;">
  <h2 class="text-center">New restaurant</h2>
  <%= simple_form_for [ @restaurant ] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category %>
  <%= f.submit "Submit restaurant", class: "btn btn-primary" %>
  <% end %>
  <br>
  <%= link_to "back", restaurants_path, class: "btn btn-secondary"%>
</div>
